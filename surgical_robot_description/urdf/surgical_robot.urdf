<joint name="joint11" type="prismatic">
    <parent link="horizontal_long"/>
    <child link="needle_driver"/>
    <pose> 0 0 0 0 0 0</pose>    
    <origin xyz="0.16 0.0185 -0.01508" rpy="0 0 0" />
    <limit lower="0" upper="0.1" effort="0.001" velocity="0.2" />
    <axis xyz= "0 1 0"/>
</joint>

<joint name="joint12" type="prismatic">
    <parent link="horizontal_short"/>
    <child link="needle_driver"/>
    <pose> 0 0 0 0 0 0</pose>    
    <origin xyz="0.16 0 -0.00719" rpy="0 0 0" />
    <limit lower="0" upper="0.1" effort="0.001" velocity="0.2" />
    <axis xyz= "0 1 0"/>
</joint>

<joint name="joint13" type="revolute">
    <parent link="needle_driver"/>
    <child link="needle_link"/>
    <pose> 0 0 0 0 0 0</pose>    
    <origin xyz="0 -0.03 -0.03719" rpy="1.57 0 0" />
    <limit effort="1000.0" lower="-0.5" upper="0.5" velocity="0.5"/> 
    <axis xyz= "0 1 0"/>
    <mimic joint="joint7" multiplier="1" offset="0"/>
</joint>

<joint name="joint14" type="fixed">
    <parent link="needle_link"/>
    <child link="needle_eef"/>
    <pose> 0 0 0 0 0 0</pose>    
    <origin xyz="0 0 0.06125" rpy="0 0 0" />
</joint>
