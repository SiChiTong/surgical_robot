    <link name="needle_link">
      <collision>
        <geometry>
          <cylinder length="0.12" radius="0.0022"/>
        </geometry>
      </collision>
     <visual name="needle_link_visual">
        <geometry>
          <cylinder length="0.12" radius="0.0022"/>
        </geometry>
        <material name="green"/>
      </visual>
    </link>     

    <link name="needle_eef">
